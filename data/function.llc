let pi = 3;

func factorial int -> int {
    factorial(0) = 1;
    factorial(n) = mul(n, factorial(sub(n, 1)));
}

func circle_area int -> int {
    circle_area(radius) = mul(pi, mul(radius, radius));
}

func five void -> int {
    5;
}

func main void -> int {
    circle_area(five()); // Example usage of circle_area
}